{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "Bundler", 
    "strict": true,
    "types": ["k6"],
    "lib": ["ESNext", "dom"],
    "typeRoots": ["./types", "./node_modules/@types"],
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
